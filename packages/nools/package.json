{
	"name": "@nools/main",
	"version": "2.3.8",
	"description": "nools in TypeScript.",
	"main": "nools.min.js",
	"scripts": {
		"test1": "mocha -R dot --require should ./tests/**.test.js --max-old-space-size=4096 --harmony --check-leaks",
		"converage1": "istanbul cover -hook-run-in-content node_modules/mocha/bin/_mocha --require should ./tests/**.test.js --max-old-space-size=4096 --harmony --check-leaks",
		"dist1": "gulp && npm publish ./dist/",
		"build1": "gulp",
		"karma1": "karma start ./tests/karma.unit.js",
		"build": "tsc -b .",
		"start": "tsc -w --preserveWatchOutput"
	},
	"keywords": [
		"nools"
	],
	"author": "taoqf <tao_qiufeng@126.com>",
	"license": "MIT",
	"devDependencies": {
		"typescript": "3.9.7",
		"@nools/parser": "*",
		"@nools/data-structures": "*",
		"@nools/lodash-port": "*"
	},
	"peerDependencies": {
		"@nools/parser": "*",
		"@nools/data-structures": "*",
		"@nools/lodash-port": "*"
	},
	"dependencies": {
		"@types/node": "14.0.27",
		"lodash-ts": "1.2.7",
		"should": "13.2.3"
	}
}
