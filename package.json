		"@types/mocha": "5.2.5",
		"babel-loader": "8.1.0",
		"browserify": "16.5.2",
		"gulp": "4.0.2",
		"gulp-babel": "8.0.0",
		"gulp-rename": "2.0.0",
		"gulp-shell": "0.8.0",
		"gulp-uglyfly": "1.4.2",
		"istanbul": "0.4.5",
		"karma": "5.1.1",
		"karma-chrome-launcher": "3.1.0",
