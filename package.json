{
	"name": "nools-ts",
	"version": "2.3.8",
	"description": "nools in TypeScript.",
	"main": "nools.min.js",
	"private": true,
	"workspaces": [
		"packages/*"
	],
	"scripts": {
		"test": "mocha -R dot --require should ./tests/**.test.js --max-old-space-size=4096 --harmony --check-leaks",
